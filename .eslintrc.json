{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "no-param-reassign": "off",
    "comma-dangle": "off",
    "prefer-template": "off",
    "import/extensions": "off",
    "class-methods-use-this": "off",
    "operator-linebreak": "off"
  }
}
